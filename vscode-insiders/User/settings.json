  "projectManager.groupList": true,
  "redhat.telemetry.enabled": false,
  "sortJSON.excludedFiles": ["package.json", "**/package.json"],
  "svelte.enable-ts-plugin": true,
  "svg.preview.mode": "svg",
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules/**",
    "**/vendor/**",
    "**/.github/**"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "DISCUSS",
    "FIXIT",
    "FIXME",
    "TODO",
    "VOTE",
    "[ ]",
    "[x]"
  ],
  "todo-tree.general.tagGroups": {
    "BUG": ["BUG", "FIXIT", "FIXME"]
  },
  "todo-tree.highlights.defaultHighlight": {
    "icon": "check",
    "type": "none"
  },
  "todo-tree.highlights.customHighlight": {
    "BUG": { "icon": "bug", "iconColour": "#aa0000" },
    "DISCUSS": { "icon": "organization", "iconColour": "#add8e6" },
    "TODO": { "icon": "tasklist", "iconColour": "#ffa500" }
  },
  "vsicons.dontShowNewVersionMessage": true,
  "wallaby.codeLensFeature.runTest": true,
  "wallaby.startAutomatically": false,
  "cSpell.enableFiletypes": ["po", "postcss", "vue", "xml"],
